# Default empty server
server {
  listen <%= ENV['PORT'] %> default_server;
  server_name _;
  return 200;
}

server {
  server_name workinfrance.fr workinfrance.beta.gouv.fr;
  listen <%= ENV['PORT'] %>;
  rewrite ^/(.*)$ https://work-in-france.incubateur.social.gouv.fr/$1 permanent;
}

server {
  server_name mes-aides.gouv.fr;
  listen <%= ENV['PORT'] %>;
  rewrite ^/(.*)$ https://www.mesdroitssociaux.gouv.fr/;
}


server {
  server_name webconf.beta.gouv.fr;
  listen <%= ENV['PORT'] %>;
  rewrite ^/(.*)$ https://webconf.numerique.gouv.fr/;
}
